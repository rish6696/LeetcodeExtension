{
    "name":"Sort Questions",
    "version":"1.0",
    "manifest_version":2,
    "content_scripts":[
        {
            "matches":["https://leetcode.com/tag/*/"],
            "js":["content.js"]
        }
    ],
    "browser_action":{
        "default_popup":"popup.html",
        "default_title":"Sort Leetcode Questions"
    }
}